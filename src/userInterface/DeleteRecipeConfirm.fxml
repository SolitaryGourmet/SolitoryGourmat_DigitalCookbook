            <Label layoutX="21.0" layoutY="54.0" text="Are you sure to delete this recipe?">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <Button fx:id="delete_Button" layoutX="57.0" layoutY="121.0" mnemonicParsing="false" text="Delete recipe">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="cancel_Button" layoutX="219.0" layoutY="121.0" mnemonicParsing="false" text="Cancel">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
